    def le_envia_comando(self):        
        while True:
            comando = input('Digite o comando desejado (ou -1 fechar o programa sair): \n')

            if comando == '-1':
                self.__rodando = False
                return

            resposta = self.__bot.executa_comando(comando)

            if resposta is None:
                print('Comando inexistente')
            else:
                print(resposta)
                return
